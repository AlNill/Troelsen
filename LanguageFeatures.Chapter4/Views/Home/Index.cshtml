@{
    Layout = null;
}
@model IEnumerable<string>;

<!DOCTYPE html>

<html>
    <head>
        <meta name="viewpoint" content="width=device-width"/>
        <title>Language Features</title>
    </head>
    <body>
        <ul>
        @foreach (string s in Model)
        {
            <li>@s</li>
        }
        </ul>
    </body>
</html>